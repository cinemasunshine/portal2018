parameters:
	level: 0
	bootstrap: %currentWorkingDirectory%/src/bootstrap.php
	paths:
		- %currentWorkingDirectory%/src
	excludes_analyse:
		- %currentWorkingDirectory%/src/ORM/Proxy/*

	ignoreErrors:
		# ライブラリの改修が必要なので除外する
		-
			message: '#::__construct\(\) does not call parent constructor from#'
			path: %currentWorkingDirectory%/src/Schedule/Entity/*
