##
## .htaccess sample
##

<IfModule mod_rewrite.c>
  RewriteEngine On
  
  # 旧サイト用ルール SASAKI-320
  RewriteRule ^sp$ / [L,R=301,QSD]
  RewriteRule ^sp/.*$ / [L,R=301,QSD]
  
  # Application Rule
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ index.php [QSA,L]
</IfModule>