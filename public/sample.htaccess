##
## .htaccess sample
##

<IfModule mod_rewrite.c>
  RewriteEngine On
  
  # 旧サイト用ルール SASAKI-313
  RewriteRule ^sp$ oldpage.html [L]
  RewriteRule ^sp/.*$ oldpage.html [L]
  
  # Application Rule
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ index.php [QSA,L]
</IfModule>